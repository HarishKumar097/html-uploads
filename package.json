{
  "name": "v2-uploads",
  "version": "1.3.2",
  "description": "A component for uploading files, designed for integration with FastPix Direct Uploads",
  "main": "./dist/uploads.js",
  "type": "module",
  "module": "./dist/uploads.js",
  "browser": "./dist/uploads.js",
  "unpkg": "./dist/uploads.js",
  "exports": {
    ".": {
      "import": "./dist/uploads.js",
      "require": "./dist/uploads.js",
      "default": "./dist/uploads.js"
    }
  },
  "scripts": {
    "build": "esbuild src/index.ts --target=es2019 --bundle --outfile=./dist/uploads.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "author": "FastPix, Inc",
  "license": "MIT",
  "dependencies": {
    "@fastpix/uploader": "^1.0.3",
    "event-target-shim": "^6.0.2",
    "xhr": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "esbuild": "^0.25.4",
    "typescript": "^5.6.2"
  }
}
